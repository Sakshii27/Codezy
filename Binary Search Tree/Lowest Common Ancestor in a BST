Node* LCA(Node *root, int n1, int n2)
{
    int ans;
   //Your code here
   if(root == NULL) return root;
   
   if(root->data >n1 && root->data >n2){
       // left meh jana h 
       return LCA(root->left,n1,n2);
   }
   else if(root->data <n1 && root->data <n2){
       return LCA(root->right,n1,n2);
       
   }
   else{
        return root;
   }
}
